{
  "name": "示例插件",
  "version": "1.0.0",
  "author": "SecRandom Team",
  "description": "这是一个功能完整的示例插件，展示了插件开发的最佳实践，包含配置管理、数据存储、多种操作功能和丰富的UI组件",
  "icon": "plugins/示例插件/assets/icon.png",
  "category": "示例",
  "tags": ["示例", "教程", "开发", "演示"],
  "min_app_version": "1.0.0",
  "dependencies": [],
  "permissions": [
    "config_read",
    "config_write",
    "data_read",
    "data_write",
    "ui_display"
  ],
  "features": {
    "multi_page": true,
    "configurable": true,
    "data_persistent": true,
    "statistics": true,
    "theme_aware": true
  },
  "supported_operations": [
    "hello",
    "calculate",
    "process_text"
  ],
  "ui_components": [
    "tab_widget",
    "card_widget",
    "form_layout",
    "text_edit",
    "combo_box",
    "spin_box",
    "check_box",
    "push_button"
  ],
  "file_structure": {
    "main": "main.py",
    "page": "page.py",
    "config": "config/plugin_config.json",
    "data": "data/plugin_data.json",
    "readme": "README.md"
  }
}